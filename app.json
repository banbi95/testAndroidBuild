{
  "expo": {
    "name": "testbuild",
    "slug": "testbuild",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fit-logo.png",
    "scheme": "testbuild",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fit-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.devin91.testbuild"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/fit-logo-pure.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "26c57684-5321-43c5-99d1-4d723929718e"
      }
    },
    "owner": "devin91"
  }
}
